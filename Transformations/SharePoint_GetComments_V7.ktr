    <connection>RAPPORT_DEV</connection>
    <sql>SELECT DISTINCT  
	APP_FKEY,
	MAX( ShippingComments_KEY ) AS MaxShippingcomments_key 
FROM 
	Rapport..RPT_CO RPT_CO
	LEFT OUTER JOIN Rapport..RPT_ShippingComments RPT_SHIPPI
	ON 
		RPT_CO.CO_KEY = RPT_SHIPPI.CO_FKEY
	 
WHERE 
	ShippingComments_UD_SC_SHIPPING_COMMENT IS NOT NULL 

GROUP BY 
	APP_FKEY </sql>
