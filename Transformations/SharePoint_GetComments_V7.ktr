    <connection>RAPPORT_DEV</connection>
    <sql>SELECT DISTINCT  
	APP_KEY,
	APP_CONTRACT_NUMBER,
	CO_custName,
	CO_custCity,
	CO_custState,
	CO_equipmentCost,
	CO_programType_TBDESC,
	CO_branch_TBDESC,
	CO_UD_CO_APP_STATUS_TBDESC,
	DEFAULT_DESC,
	APP_FOLLOW_UP_DATE,
	MAX( REP1 ) AS MaxRep1,
	MAX( REP2 ) AS MaxRep2,
	MAX( REP3 ) AS MaxRep3 
FROM 
	Rapport..APPLICATION APPLICATIO
	INNER JOIN Rapport..RPT_CO RPT_CO
	ON 
		APPLICATIO.APP_KEY = RPT_CO.APP_FKEY
	LEFT OUTER JOIN Rapport..Marketing_Reps MARKETING
	ON 
		RPT_CO.CO_KEY = MARKETING.CO_FKEY
	INNER JOIN Rapport..CHANGE_HISTORY CHANGE_HIS
	ON 
		APPLICATIO.APP_KEY = CHANGE_HIS.CHG_APP_FKEY
	INNER JOIN Rapport..DECISION_CODES DECISION_C
	ON 
		APPLICATIO.APP_DECISION_CODE = DECISION_C.DECISION_CODE_KEY
	INNER JOIN Rapport..RPT_ASSET RPT_ASSET
	ON 
		RPT_CO.CO_KEY = RPT_ASSET.CO_FKEY
	 
WHERE 
	CHG_FIELD_NAME = 'UD_CO_APP_STATUS'
	AND CO_equipmentCost > 10000
	AND CO_UD_CO_APP_STATUS_TBDESC IN( 'Order Released', 'Order Submitted', 'Docs Received - Review', 'Docs Back - Need Credit Approval', 'Docs Back - Missing Information' )
	AND DEFAULT_DESC NOT IN( 'Booked', 'Cancelled', 'Rejected' )
	AND CO_programType_TBDESC IN( 'FPP LEASE', 'SMALL TICKET PROGRAM', 'CPP LEASE', 'FPP TVO', 'CPP TVO', 'VA LEASE PROGRAM' ) 
	AND CHG_DATE_TIME > '9/15/2018'

GROUP BY 
	APP_KEY,
	APP_CONTRACT_NUMBER,
	CO_custName,
	CO_custCity,
	CO_custState,
	CO_equipmentCost,
	CO_programType_TBDESC,
	CO_branch_TBDESC,
	CO_UD_CO_APP_STATUS_TBDESC,
	DEFAULT_DESC,
	APP_FOLLOW_UP_DATE 



ORDER BY CO_UD_CO_APP_STATUS_TBDESC  DESC
      
</sql>
